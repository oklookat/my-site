{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"lib": ["ES2021"],
		"paths": {
			// default
			"$lib":["src/lib"],
			"$lib/*": [
				"src/lib/*"
			],

			// custom
			"$lib_elven":["src/lib_elven"],
			"$lib_elven/*": [
				"src/lib_elven/*"
			],
			//
			"$lib_oklookat":["src/lib_oklookat"],
			"$lib_oklookat/*": [
				"src/lib_oklookat/*"
			]
		}
	}
}